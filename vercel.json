{
  "version": 2,
  "routes": [
    {
      "src": "/api/auth/callback",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Authorization, Content-Type"
      },
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/api/.*",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Authorization, Content-Type"
      },
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "methods": ["GET"]
    }
  ]
}
