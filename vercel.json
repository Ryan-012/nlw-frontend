{
  "version": 2,
  "routes": [
    {
      "src": "/api/.*",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET,OPTIONS",
        "Access-Control-Allow-Headers": "Authorization,Content-Type"
      },
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/memories/:path*",
      "continue": true,
      "dest": "/memories"
    },
    {
      "src": "/users/:path*",
      "continue": true,
      "dest": "/users"
    },
    {
      "src": "/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "methods": ["GET"]
    }
  ]
}
